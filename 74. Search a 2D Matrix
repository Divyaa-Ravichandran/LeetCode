


URL: https://leetcode.com/problems/search-a-2d-matrix/description/

class Solution:
    def searchMatrix(self, matrix: List[List[int]], target: int) -> bool:
        
        a=[[0,1],[1,0],[0,-1],[-1,0]]
        m=len(matrix)
        n=len(matrix[0])
        for i in range(m):
            for j in range(n):
                if(matrix[i][j]==target):
                    return True
                else:
                    continue

        
        
