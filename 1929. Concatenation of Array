PYTHON


class Solution:
    def getConcatenation(self, nums: List[int]) -> List[int]:
        '''
        n=len(nums)
        r=[]
        for i in range(0,2*n):
            if i<n:
                r.append(nums[i])
            else:
                r.append(nums[i-n])
        return r
'''

        return nums*2



OR

JAVA


class Solution {
    public int[] getConcatenation(int[] nums) {
        int i=0,n=nums.length;
        int m=2*n;
        int[] ans=new int[n*2];

      

        for(i=0;i<m;i++)
        {
            if (i<n)
            ans[i]=nums[i];
            else
            ans[i]=nums[i-n];
        }
        return ans;
    }
}



