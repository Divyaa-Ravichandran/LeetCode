

URL: https://leetcode.com/problems/valid-parentheses/


class Solution:
    def isValid(self, s: str) -> bool:
        stack=[]
        
        for char in s:

            if char=='(' or char=='[' or char=='{':
                stack.append(char)
            else:
                if not stack:
                    return False
                currentchar=stack.pop()
                if currentchar=='(':
                    if char!=')':
                        return False
                if currentchar=='[':
                    if char!=']':
                        return False
                if currentchar=='{':
                    if char!='}':
                        return False
                
            
        if stack:
            return False
        return True
            
