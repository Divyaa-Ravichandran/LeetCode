
URL: https://leetcode.com/problems/remove-element/


class Solution:
    def removeElement(self, nums: List[int], val: int) -> int:
        #while val in nums: nums.remove(val)
        i = 0
        for x in nums:
            if x != val:
                nums[i] = x
                i += 1
        return i
        
        
        
OR



class Solution:
    def removeElement(self, nums: List[int], val: int) -> int:
        while val in nums: nums.remove(val)
